<root>
  <update>
    <blend type="opaque" />
    <material max_insts="2048" vertex_shader="Default2DVertexShader" pixel_shader="PSFootprintUpdate">
      <constant_buffers>
        <item shader_type="2">
          <param name="h0" type="float" data="0.1" />
          <param name="h1" type="float" data="0.05" />
          <param name="k" type="float" data="0.2" />
          <param name="texofs" array_index="0" type="float2" data="0,1" />
          <param name="texofs" array_index="1" type="float2" data="1,0" />
          <param name="texofs" array_index="2" type="float2" data="0,-1" />
          <param name="texofs" array_index="3" type="float2" data="-1,0" />
        </item>
      </constant_buffers>
      <samplers>
        <item name="LinearSampler" />
      </samplers>
    </material>
  </update>
  <render>
    <blend type="transparent1" />
    <material max_insts="2048" vertex_shader="Default2DVertexShader" pixel_shader="g_COneTexturePixelShader">
      <samplers>
        <item name="LinearSampler" />
      </samplers>
    </material>
  </render>

  <update1>
    <blend type="opaque" />
    <material max_insts="2048" vertex_shader="Default2DVertexShader" pixel_shader="g_COneTexturePixelShader">
      <samplers>
        <item name="LinearSampler" />
      </samplers>
    </material>
  </update1>
  <render1_color>
    <blend type="transparent" />
    <material max_insts="2048" vertex_shader="Default2DVertexShader" pixel_shader="PSFootprintLiquidColor">
      <shader_resources>
        <item name="TextureColorMap" filename="textures/colormap.png|nomip" />
      </shader_resources>
      <samplers>
        <item name="LinearSampler" />
      </samplers>
    </material>
  </render1_color>
  <render2_color>
    <blend type="transparent" />
    <material max_insts="2048" vertex_shader="Default2DVertexShader" pixel_shader="PSFootprintLiquidColor">
      <shader_resources>
        <item name="TextureColorMap" filename="textures/colormap1.png|nomip" />
      </shader_resources>
      <samplers>
        <item name="LinearSampler" />
      </samplers>
    </material>
  </render2_color>
  <!--render1_occlusion>
    <blend type="opaque" />
    <material max_insts="2048" vertex_shader="Default2DVertexShader" pixel_shader="PSFootprintLiquidOcclusion">
      <shader_resources>
        <item name="TextureColorMap" filename="textures/colormap.tga" />
      </shader_resources>
      <samplers>
        <item name="LinearSampler" />
      </samplers>
    </material>
  </render1_occlusion-->
</root>